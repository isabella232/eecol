{
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "eslint.validate": [
        "javascript"
    ],
    "stylelint.enable": true,
    "[css]": {
        "editor.defaultFormatter": "stylelint.vscode-stylelint"
    },
    "todohighlight.isEnable": true,
    "todohighlight.keywords": [
        "TODO:",
        "KLUDGE:",
        "NOTE:",
        "HACK:"
    ]
}